<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.dtchain.dao.WorkTimeDao">

	<insert id="addPunchInfo" parameterType="io.dtchain.entity.WorkTimeBean">
		insert into workTimeTable (empName,dept,dates,workMorn,atNoon,workAfter,atNight) VALUES(#{empName},#{dept},#{dates},#{workMorn},#{atNoon},#{workAfter},#{atNight});
	</insert>
</mapper>